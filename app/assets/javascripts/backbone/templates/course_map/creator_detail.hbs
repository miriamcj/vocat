<header data-behavior="sticky-header" class="matrix--overlay--header">
  <h2 class="matrix--overlay--title">{{creator.name}}</h2>
  <a href="#" class="matrix--overlay--close" data-behavior="matrix-overlay-close">Close</a>
  <nav class="project-selector" data-behavior="dropdown">
    <a class="project-selector--toggle" data-behavior="toggle" href="#">Select a project</a>
    <div class="project-selector--dropdown" data-behavior="dropdown-options">
      <ul>
        {{#each projects}}
          <li><a data-behavior="routable" href="{{url_for 'course_evaluations_for_creator_and_project' ../courseId ../creator.id this.id}}"><strong>{{this.name}}</strong></a></li>
        {{/each}}
      </ul>
    </div>
  </nav>
</header>
<div data-behavior="submission-summaries"></div>
