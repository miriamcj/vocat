<div class="matrix--row-header--item">
  <form>
    <fieldset>
      <div class="input-group">
        <input style="margin-bottom: 10px; width: 150px;" class="text-input inline" value="{{name}}" data-behavior="name" />
        <a class="small-teal-minus-button" href="#" data-behavior="destroy" style="display: inline-block; margin-right: 15px; height: 26px; width: 36px; padding-left: 3px; "></a>
        <span data-behavior="low">{{low}}</span> <small>&ndash;</small> <span data-behavior="high">{{high}}</span>
      </div>
    </fieldset>
 </form>
</div>
