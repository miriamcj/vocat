<div data-behavior="toggle-evaluation-detail">
{{{include "range" title=this.title percentage=this.percentage range_class="range-total"}}}
</div>

<ul class="evaluation-single--details evaluations-hidden" data-behavior="detail-child-container">
  {{#each score }}
  <li>
    {{{include "range" title=@key percentage=this.percentage range_class="range-field"}}}
  </li>
  {{/each}}
</ul>
