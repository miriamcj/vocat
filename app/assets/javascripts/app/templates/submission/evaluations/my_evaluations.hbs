
<li data-model-id="{{id}}" class="evaluation-collection evaluation-collection-instructor">
	<div data-behavior="toggle-children">
		<div data-behavior="range" class="range range-expandable">
			<h4 class="range--title">
				Your Evaluation
			</h4>
			<div class="range--track">
				<span style="width: 50%" class="range--fill"></span>
				<div class="range--handle"><b></b><span data-container="percentage">{{total_percentage_rounded}}</span>%</div>
			</div>
		</div>
	</div>
	<ul class="evaluation-single--details evaluations-hidden" data-behavior="child-container">
		<!-- criteria will be inserted here -->
		<li data-behavior="subtotal">
			<div class="range-tertiary range-editable range-total">
				<div class="range--track-wrapper">
					<h4 class="range--total-label">Rubric Total</h4>
					<div class="range--handle">
						<div class="score-wrapper">
							<span class="score" data-container="subtotal-score">0</span>
							<span class="slash"></span>
							<span class="possible">{{points_possible}}</span>
						</div>
					</div>
				</div>
			</div>
		</li>
		<li data-behavior="total">
			<div class="range-tertiary range-editable range-total">
				<div class="range--track-wrapper">
					<h4 class="range--total-label">Weighted Score</h4>
					<div class="range--handle">
						<div class="score-wrapper">
							<span class="score" data-container="total-score">0</span>
							<span class="slash"></span>
							<span class="possible">{{points_possible}}</span>
						</div>
					</div>
				</div>
			</div>
		</li>
		<li data-behavior="utility" class="evaluation-single--utility">
			<a data-behavior="evaluation-publish" class="primary-button show-button" href="#">Publish Evaluation</a>
			<a data-behavior="evaluation-unpublish" class="secondary-button unpublish-button" href="#">Unpublish Evaluation</a>
			<a data-behavior="evaluation-destroy" class="warning-button delete-button" href="#">Delete Evaluation</a>
		</li>
	</ul>
</li>
