<ul class="scores--summary-list">
  {{#if submission.instructor_score_percentage}}
  <li class="scores--evaluated">          
    <h3 class="scores--score-title">Evaluator Review</h3>
    <div class="indicator" style="width: {{ to_i submission.instructor_score_percentage }}%;">
      <span class="scores--score-value">{{ to_i submission.instructor_score_percentage }}<small>%</small></span>
    </div>
  </li>
  {{else}}
  <li class="scores--unevaluated">
    <h3 class="scores--score-title">Evaluator Review</h3>
    <div class="indicator">
      <span class="scores--score-value">&ndash;&ndash;&nbsp;&nbsp;</span>
    </div>
  </li>
  {{/if}}
  {{#if submission.peer_score_percentage}}
  <li class="scores--evaluated">
    <h3 class="scores--score-title">Peer Review</h3>
    <div class="indicator" style="width: 82%;">
      <span class="scores--score-value">82<small>%</small></span>
    </div>
  </li>
  {{else}}
  <li class="scores--unevaluated">
    <h3 class="scores--score-title">Peer Review</h3>
    <div class="indicator">
      <span class="scores--score-value">&ndash;&ndash;&nbsp;&nbsp;</span>
    </div>
  </li>
  {{/if}}
</ul>
