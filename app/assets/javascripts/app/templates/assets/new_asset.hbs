<div class="page-section--component-header strong centered">This Project Accepts Videos, Images, and Audio</div>

	<div class="page-section--component asset-upload">
		<div class="asset-uploading" data-behavior="asset-uploading-message" style="display: none">
			Uploading...
		</div>
			<input type="hidden" name="AWSAccessKeyId" value="{{AWSPublicKey}}" />
			<input type="hidden" name="acl" value="private" />
			<input type="hidden" name="success_action_status" value="200" />
			<input type="hidden" name="key" />
			<input type="hidden" name="policy" />
			<input type="hidden" name="signature" />
			<form data-behavior="upload-form" accept-charset="UTF-8" action="http://{{s3Bucket}}.s3.amazonaws.com" data-remote="true" enctype="multipart/form-data" id="video-upload" method="post">
			<div class="page-section-one-third asset-upload--file">
				<div class="wrapper">
				<span class="file-input-container">
						<input  data-behavior="file-input" name="file" type="file" />
						<a href="#" data-behavior="file-input-trigger" class="primary-button new-button">Select File to Upload</a>
					</span>
				</div>
			</div>
			<div data-behavior="dropzone" class="page-section-one-third asset-upload--target">
				<div class="wrapper">
						<a href="#" class="decorated" data-behavior="file-picker">
							Drag and Drop
						</a>
					</span>
				</div>
			</div>
			</form>
			<form data-behavior="upload-form" accept-charset="UTF-8" id="video-external" method="post">
			<div class="page-section-one-third asset-upload--input">
				<div class="wrapper">
					<div class="formgrid-input">
						<input type="text" data-behavior="" placeholder="Vimeo / YouTube URL" />
					</div>
				</div>
			</div>
			</form>
	</div>
	<div class="centered asset-done">
		<a href="#" data-behavior="hide-manage" class="secondary-button close-button">Done</a>
	</div>
