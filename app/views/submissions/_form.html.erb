<%= form_for([@organization, @course, @assignment, @submission]) do |f| %>
  <%= form_errors(@submission) %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :summary %><br />
    <%= f.text_area :summary %>
  </div>
  <div class="actions">
    <%= f.submit "Continue" %>
  </div>
<% end %>

