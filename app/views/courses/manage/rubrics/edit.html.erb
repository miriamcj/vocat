<% content_for :content_header do %>
  Edit Rubric
<% end %>

<% content_for :content_header_links do %>
  <% if @course %>
    <%= link_to 'Back to List', course_manage_rubrics_path(@course), :class => 'teal-arrow-button' %>
  <% else %>
    <%= link_to 'Back to List', rubrics_path(), :class => 'teal-arrow-button' %>
  <% end %>
<% end %>

<% content_for :content_intro do %>
<% end %>

<% content_for :content_frames	 do %>
  <div style="padding-top: 0" class="page-content--frame double-space-bottom">
    <header class="button-header">
      <h2 class="button-header--title"><%= @rubric.name %></h2>
  </div>
  <div id="region-main"></div>
  <%= render :partial => 'shared/bootstrap_model', :locals => {:key => 'Rubric', :model => @rubric}%>
<% end %>


