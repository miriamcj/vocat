<% content_for :content do %>
  <div class="page-content--heading">
    <%= link_to 'Back to Your Submissions', portfolio_course_path(@selected_course), :class => 'header-link back-link' %>
    <h2 class="heading-primary">
      <%= @project.name %>
    </h2>
    <% if @creator != current_user %>
    <h2 class="heading-secondary">
      <%= @creator.name %>
    </h2>
    <% end %>
  </div>
  <div data-hide-back-link id="region-main"></div>
  <%= render :partial => 'shared/bootstrap_collection', :locals => {:type => 'Collections', :key => 'user', :data => [@creator] }%>
  <%= render :partial => 'shared/bootstrap_collection', :locals => {:type => 'Collections', :key => 'project', :data => [@project]}%>
<% end %>
